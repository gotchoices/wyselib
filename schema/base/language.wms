require base.tcl

# Table of languages
#----------------------------------------------------------------
table base.language {base} {
    code	varchar(3)	primary key
  , iso_3b	text		unique
  , eng_name	text		not null
  , fra_name	text		not null
  , iso_2	varchar(2)	unique
}
